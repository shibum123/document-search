  "private": true,
  "dependencies": {
    "elasticsearch": "^15.2.0",
    "font-awesome": "^4.7.0",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "sass-loader": "^8.0.2"
  },
  "scripts": {
